   	  equipmentId = #{equipmentId,jdbcType=INTEGER},
      userMan = #{userMan,jdbcType=VARCHAR},
      repairMan = #{repairMan,jdbcType=VARCHAR},
      repairTime = #{repairTime,jdbcType=VARCHAR},
      finishTime = #{finishTime,jdbcType=VARCHAR},
    	  state = #{state,jdbcType=INTEGER},
       finishState = #{finishState,jdbcType=INTEGER}
    where id = #{id,jdbcType=INTEGER}
  </update>
	
	 <update id="updateByPrimaryKeySelective" parameterType="com.chen.model.Repair" >
